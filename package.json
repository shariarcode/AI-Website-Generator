    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-simple-code-editor": "^0.13.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.4"
  }
}