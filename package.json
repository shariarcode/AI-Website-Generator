  "scripts": {
    "build": "mkdir -p dist && cp index.html dist/ && esbuild index.tsx --bundle --outfile=dist/bundle.js --loader:.tsx=tsx --define:process.env.API_KEY=\\\"$API_KEY\\\" --minify --sourcemap"
  },
  "dependencies": {
    "@google/genai": "^1.13.0",
